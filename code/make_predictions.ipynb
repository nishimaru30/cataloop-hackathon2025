
This notebook demonstrates how to use our trained models
to make predictions on new reactions.
"""

import pandas as pd
import numpy as np
from predict import predict_yield, predict_ee

# Load example data
test_df = pd.read_csv('../data/test_data.csv')

print("Test data:")
print(test_df.head())

# Make predictions
print("Making predictions...")
yield_predictions = predict_yield(test_df)
ee_predictions = predict_ee(test_df)

# Add to dataframe
test_df['Predicted_Yield (%)'] = yield_predictions
test_df['Predicted_ddG_er (kcal/mol)'] = ee_predictions

print("\nPredictions:")
print(test_df[['Reactant SMILES', 'Predicted_Yield (%)', 'Predicted_ddG_er (kcal/mol)']].head())

# Save
test_df.to_csv('predictions.csv', index=False)
print("Predictions saved!")
